{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Tenant</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Rg</th>
                <td>{{ entity.rg }}</td>
            </tr>
            <tr>
                <th>Cpf</th>
                <td>{{ entity.cpf }}</td>
            </tr>
            <tr>
                <th>Phoneres</th>
                <td>{{ entity.phoneRes }}</td>
            </tr>
            <tr>
                <th>Phonecom</th>
                <td>{{ entity.phoneCom }}</td>
            </tr>
            <tr>
                <th>Phonecel</th>
                <td>{{ entity.phoneCel }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ entity.status }}</td>
            </tr>
            <tr>
                <th>Street</th>
                <td>{{ entity.street }}</td>
            </tr>
            <tr>
                <th>Number</th>
                <td>{{ entity.number }}</td>
            </tr>
            <tr>
                <th>Complement</th>
                <td>{{ entity.complement }}</td>
            </tr>
            <tr>
                <th>Neighborhood</th>
                <td>{{ entity.neighborhood }}</td>
            </tr>
            <tr>
                <th>Cityid</th>
                <td>{{ entity.cityId }}</td>
            </tr>
            <tr>
                <th>Stateid</th>
                <td>{{ entity.stateId }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ entity.comment }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{{ entity.updatedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('tenant') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('tenant_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
